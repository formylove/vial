<?xml version="1.0" encoding="UTF-8"?>
<configuration status="OFF">
    <appenders>
        <File name="log" fileName="logs/test.log" append="false">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} %-5level %class{36} %L %M - %msg%xEx%n"/>
        </File>
    </appenders>
    <loggers>
        <root level="trace">
            <appender-ref ref="log"/>
        </root>
    </loggers>
</configuration>